@model IEnumerable<Library.Model.Log>

@{
    ViewBag.Title = "Library Logs";
    Layout = null;
}
@Styles.Render("~/Content/css")
<h2>View</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RequestMethod)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RequestUri)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsAuthenticated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserHostName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserHostAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserAgent)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResponseStatusCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResponseError)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RequestDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RequestMethod)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RequestUri)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsAuthenticated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserHostName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserHostAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserAgent)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ResponseStatusCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ResponseError)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RequestDate)
        </td>
    </tr>
}

</table>
